        "warningForeground": "primaryForeground",
        "warningBackground": "menuBackground",
        "warningBorderColor": "warning",
        "errorForeground": "primaryForeground",
        "errorBorderColor": "error",
        "errorBackground": "menuBackground",
        "informativeForeground": "primaryForeground",
        "informativeBackground": "menuBackground",
        "informativeBorderColor": "accentColor"
      }
    },
    "NotificationsToolwindow": {
      "newNotification": {
        "hoverBackground": "hoverBackground",
        "background": "menuBackground"
      },
      "Notification": {
        "hoverBackground": "hoverBackground"
      }
    },
    "PasswordField": {
      "background": "inputBackground"
    },
    "Plugins": {
      "SearchField": {
        "background": "editorBackground"
      },
      "SectionHeader": {
        "foreground": "primaryForeground"
      },
      "hoverBackground": "hoverBackground",
      "lightSelectionBackground": "selectionBackground",
      "Button": {
        "installBorderColor": "accentColor",
