      "endBackground": "selectionBackground",
      "focusedBorderColor": "focusAccentColor",
      "disabledBorderColor": "selectionBackground",
      "default": {
        "foreground": "primaryForeground",
        "startBackground": "selectionBackground",
        "endBackground": "selectionBackground",
