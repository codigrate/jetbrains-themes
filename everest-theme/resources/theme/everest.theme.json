      "background": "menuBackground",
      "shortcutForeground": "accentColor"
    },
    "EditorTabs": {
      "background": "menuBackground",
      "hoverBackground": "tabHoverBackground",
      "underlinedTabBackground": "tabColor",
      "inactiveUnderlineColor": "tabLineColor",
      "underlineColor": "tabLineColor",
      "underlineHeight": 3
    },
    "Label": {
      "errorForeground": "error"
