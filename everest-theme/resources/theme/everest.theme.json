{
  "name": "Everest",
  "dark": false,
  "author": "Codigrate",
  "editorScheme": "/theme/everest.xml",
  "colors": {
    "borderColor": "#c5d1d7",
    "editorBackground": "#fdfeff",
    "menuBackground": "#e4ecef",
    "sidePanelBackground": "#d5e2e8",
    "headerBackground": "#c7dae1",
    "inputBackground": "#fdfeff",
    "selectionBackground": "#b6d8e5",
    "selectionInactiveBackground": "#c3dbe5",
    "searchMatchBackground": "#f6d4c8",
    "separatorColor": "#c5d1d7",
    "tabColor": "#fdfeff",
    "tabLineColor": "#ED7E5A",
    "tabHoverBackground": "#f6d4c8",
    "accentColor": "#1a536e",
    "secondaryAccentColor": "#8586a4",
    "hoverBackground": "#d5d8e7",
    "primaryForeground": "#1a536e",
    "focusAccentColor": "#9cc5d3",
    "error": "#CE5E5E",
    "errorFocused": "#cb4848",
    "warning": "#CC9D57",
    "warningFocused": "#d09136"
